

<mvc:View
  controllerName="flightui5eh.controller.Detail"
  xmlns:mvc="sap.ui.core.mvc"
  displayBlock="true"
  xmlns="sap.m">

  <Page id="detailPage" title="Flight Details" showNavButton="true" navButtonPress="oNavButton_press">


 <Table id="detailTab" items="{detailDataModel>/}">
      <headerToolbar>
        <Toolbar id="_IDGenToolba1">
          <Title id="_IDGenTitl1" text="Airlines Items" />
        </Toolbar>
      </headerToolbar>

      <columns>
        <Column id="_IDGenColum3"><Text id="_IDGenTex3" text="Carrid" /></Column>
        <Column id="_IDGenColum4"><Text id="_IDGenTex7" text="Connid" /></Column>
        <Column id="_IDGenColum5"><Text id="_IDGenTex8" text="Fldate" /></Column>
        <Column id="_IDGenColum7"><Text id="_IDGenTex9" text="Price" /></Column>
        <Column id="_IDGenColum8"><Text id="_IDGenTex10" text="Currency" /></Column>
        <Column id="_IDGenColum9"><Text id="_IDGenTex11" text="PlaneType" /></Column>
        <Column id="_IDGenColum10"><Text id="_IDGenTet12" text="SeatsMax" /></Column>
        <Column id="_IDGenColum11"><Text id="_IDGenTet13" text="SeatsOcc" /></Column>
        <Column id="_IDGenColum12"><Text id="_IDGenTet14" text="PaymentSum" /></Column>
        <Column id="_IDGenColum13"><Text id="_IDGenTet15" text="Currency" /></Column>
      </columns>

      <items>
        <ColumnListItem id="_IDGenColumnListIte1">
          <cells>
            <Text id="_IDGenTex16" text="{detailDataModel>Carrid}" />
            <Text id="_IDGenTex17" text="{detailDataModel>Connid}" />
            <!-- <Text id="_IDGenTex18" text="{detailDataModel>Fldate}" /> -->
            <Text id="_IDGenTex18" text="{ path: 'detailDataModel>Fldate', type: 'sap.ui.model.type.Date', 
                  formatOptions: {  style: 'medium'  } }" />
            <Text id="_IDGenTex19" text="{detailDataModel>Price}" />
            <Text id="_IDGenTex20" text="{detailDataModel>Currency}" />
            <Text id="_IDGenTex21" text="{detailDataModel>PlaneType}" />
            <Text id="_IDGenTex22" text="{detailDataModel>SeatsMax}" />
            <Text id="_IDGenTex23" text="{detailDataModel>SeatsOcc}" />
            <Text id="_IDGenTex24" text="{detailDataModel>PaymentSum}" />
            <Text id="_IDGenTex25" text="{detailDataModel>Currency}" />
          </cells>
        </ColumnListItem>
      </items>
    </Table>
  </Page>
</mvc:View>









